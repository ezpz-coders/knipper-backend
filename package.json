{
  "name": "knipper",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node ./bin/www",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "nodemon ./bin/www",
    "debug": "node --inspect ./bin/www",
    "prettier:fix": "prettier --write .",
    "lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": " eslint \"**/*.{js,jsx}\" --fix",
    "verify": "yarn prettier && yarn lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.8",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.28"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.0",
    "prettier-config-standard": "^4.0.0"
  }
}
